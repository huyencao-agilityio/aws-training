{
  "name": "ecommerce-cdk-practice",
  "version": "0.1.0",
  "bin": {
    "ecommerce-cdk-practice": "bin/ecommerce-cdk-practice.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "synth:testing": "ENV=local npx cdk synth",
    "deploy:testing": "ENV=local cdk deploy TestingStage/*",
    "synth:prod": "ENV=prod npx cdk synth",
    "deploy:prod": "ENV=prod npx cdk deploy",
    "synth:staging": "ENV=staging npx cdk synth",
    "deploy:staging": "ENV=staging npx cdk deploy --require-approval never"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.798.0",
    "@aws-sdk/s3-presigned-post": "^3.798.0",
    "@types/aws-lambda": "^8.10.149",
    "@types/aws-sdk": "^0.0.42",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/jwk-to-pem": "^2.0.3",
    "@types/node": "22.7.9",
    "@types/pg": "^8.11.14",
    "aws-cdk": "2.1004.0",
    "aws-sdk": "^2.1692.0",
    "jest": "^29.7.0",
    "sharp": "^0.34.1",
    "jsonwebtoken": "^9.0.2",
    "jwk-to-pem": "^2.0.7",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "~5.6.3"
  },
  "dependencies": {
    "@aws-cdk/aws-cloudfront": "^1.203.0",
    "aws-cdk-lib": "2.181.1",
    "constructs": "^10.0.0",
    "dotenv": "^16.5.0"
  }
